{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxi generate && (if exist docs rmdir /S /Q docs) && mkdir docs && xcopy /E /I /Y dist\\* docs\\ && copy .nojekyll docs\\.nojekyll /Y && rmdir /S /Q dist",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "deploy": "gh-pages -d docs"
  },
  "dependencies": {
    "@nuxt/content": "^3.4.0",
    "@nuxt/fonts": "^0.11.1",
    "@nuxt/icon": "^1.11.0",
    "@nuxt/image": "^1.10.0",
    "nuxt": "^3.16.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
